{
  "name": "core-native-boilerplate",
  "version": "1.0.4",
  "description": "A boilerplate project for the core-native library.",
  "author": "Kelvin Chu <chubillkelvin@gmail.com> (github.com/RageBill)",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "react",
    "react-native",
    "redux",
    "redux-saga",
    "typescript",
    "boilerplate",
    "core-native",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RageBill/core-native-boilerplate.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "check": "node node-script/check.js",
    "debug": "open rndebugger://set-debugger-loc?host=localhost&port=8081",
    "format": "node node-script/format.js",
    "ios": "react-native run-ios",
    "module": "node node-script/module.js",
    "native": "cd ios && pod install && cd ../android && yarn jetify && ./gradlew clean",
    "start": "react-native start --reset-cache"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.10.3",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "5.4.2",
    "@react-navigation/stack": "5.3.9",
    "core-native": "1.9.2",
    "react-native": "0.62.2",
    "react-native-device-info": "5.5.8",
    "react-native-gesture-handler": "1.6.1",
    "react-native-reanimated": "1.9.0",
    "react-native-safe-area-context": "1.0.2",
    "react-native-splash-screen": "3.2.0",
    "react-native-screens": "2.8.0",
    "react-native-v8": "0.62.2-patch.1",
    "v8-android": "8.80.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/runtime": "7.9.6",
    "@pinnacle0/eslint-plugin-pinnacle": "1.0.0",
    "@types/chalk": "2.2.0",
    "@types/fs-extra": "9.0.1",
    "@types/yargs": "15.0.5",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "axios": "0.19.2",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "4.0.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "fs-extra": "9.0.0",
    "jetifier": "1.6.5",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "2.0.5",
    "typescript": "3.8.3",
    "yargs": "15.3.1"
  }
}
