{
  "name": "core-native-boilerplate",
  "version": "1.0.5",
  "description": "A boilerplate project for the core-native library.",
  "author": "Kelvin Chu <chubillkelvin@gmail.com> (github.com/RageBill)",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "react",
    "react-native",
    "redux",
    "redux-saga",
    "typescript",
    "boilerplate",
    "core-native",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RageBill/core-native-boilerplate.git"
  },
  "scripts": {
    "ts-node": "TS_NODE_PROJECT=./script/tsconfig.json ./node_modules/.bin/ts-node",
    "android": "react-native run-android",
    "check": "yarn ts-node script/check.ts",
    "debug": "open rndebugger://set-debugger-loc?host=localhost&port=8081",
    "format": "yarn ts-node script/format.ts",
    "ios": "react-native run-ios",
    "module": "yarn ts-node script/module.ts",
    "native": "bash ./script/native.sh",
    "start": "react-native start --reset-cache"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.11.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "5.7.1",
    "@react-navigation/stack": "5.7.1",
    "core-native": "2.1.2",
    "react-native": "0.63.2",
    "react-native-device-info": "5.6.3",
    "react-native-gesture-handler": "1.7.0",
    "react-native-reanimated": "1.10.1",
    "react-native-safe-area-context": "3.1.1",
    "react-native-screens": "2.9.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-v8": "0.63.2-patch.0",
    "v8-android": "8.84.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/runtime": "7.10.5",
    "@pinnacle0/eslint-plugin": "0.0.1",
    "@types/chalk": "2.2.0",
    "@types/fs-extra": "9.0.1",
    "@types/yargs": "15.0.5",
    "@typescript-eslint/eslint-plugin": "3.7.1",
    "@typescript-eslint/parser": "3.7.1",
    "axios": "0.19.2",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "4.0.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.4",
    "eslint-plugin-react-hooks": "4.0.8",
    "fs-extra": "9.0.0",
    "jetifier": "1.6.6",
    "metro-react-native-babel-preset": "0.61.0",
    "prettier": "2.0.5",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "yargs": "15.4.1"
  },
  "resolutions": {
    "@react-native-community/cli": "4.9.0"
  },
  "engines": {
    "node": ">= 12.16"
  }
}
